#$ -cwd
#$ -q high_mem
#$ -pe mpi 32
#$ -S /bin/bash

module load salmon/v1.3.0

SMAR_TRANS=../../01_mapping/VsTranscriptome_Preliminary/infiles/Provisional_transcriptome.fasta

for i in P*; do
	SAMPLE=${i}
	echo "Processing sample ${SAMPLE}"
	INFILE=../../01_mapping/VsTranscriptome_Preliminary/${SAMPLE}/${SAMPLE}_provisional_transcriptome.bam
	
	salmon quant -t $SMAR_TRANS -l ISF -a $INFILE -p $NSLOTS \
		--gcBias -o ${SAMPLE}
done
